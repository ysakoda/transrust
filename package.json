{
  "name": "transrust",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "style": "stylelint 'src/**/*.css'",
    "style:fix": "stylelint 'src/**/*.css' --fix",
    "lint:rust": "cd src-tauri && cargo clippy",
    "format:rust": "cd src-tauri && cargo fmt",
    "audit:rust": "cd src-tauri && cargo audit",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "npm run lint && npm run style && npm run lint:rust && npm run test"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "happy-dom": "^17.1.8",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.2",
    "stylelint": "^16.15.0",
    "stylelint-config-standard": "^37.0.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.0.3"
  }
}
